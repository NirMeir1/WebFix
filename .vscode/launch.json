{
    "version": "0.2.0",
    "compounds": [
      {
        "name": "Debug FastAPI",
        "configurations": ["Run FastAPI"]
      }
    ],
    "configurations": [
      
      {
        "name": "Run FastAPI",
        "type": "debugpy",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "backend.main:app",
          "--reload"
        ],
        "justMyCode": false,
        "python": "${workspaceFolder}/backend/venv310/Scripts/python.exe",
        "env": {
          "PYTHONPATH": "${workspaceFolder}/backend"
        }
      }
    ]
  }